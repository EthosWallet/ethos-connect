{
  "name": "ethos-wallet-beta",
  "version": "0.0.44",
  "description": "Beta version of the Ethos Wallet NPM Package",
  "main": "dist/index.cjs",
  "typings": "dist/index.d.ts",
  "module": "dist/ethos-wallet-beta.esm.js",
  "license": "MIT",
  "files": [
    "README.md",
    "dist"
  ],
  "exports": {
    "import": "./dist/ethos-wallet-beta.esm.js",
    "require": "./dist/index.cjs",
    "types": "./dist/index.d.ts"
  },
  "type": "module",
  "sideEffects": false,
  "engines": {
    "node": ">=10"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EthosWallet/react-api.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prepublishOnly": "npm run build && npm test && npm run lint",
    "build": "./scripts/build.sh --external:react --external:react-dom",
    "watch": "./scripts/watch.sh --external:react --external:react-dom",
    "test": "./scripts/test.sh",
    "test:watch": "./scripts/test.sh --watch",
    "lint": "./scripts/lint.sh",
    "playground": "yarn workspace playground-react dev",
    "clean": "rimraf ./dist"
  },
  "peerDependencies": {
    "react": "^16 || ^17 || ^18",
    "react-dom": "^16 || ^17 || ^18"
  },
  "devDependencies": {
    "@swc/core": "^1.2.197",
    "@swc/jest": "^0.2.21",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.0.0",
    "@types/jest": "^28.1.0",
    "@types/react": "^17.0.43",
    "@types/react-dom": "^17.0.14",
    "esbuild": "^0.11.18",
    "fast-glob": "^3.2.11",
    "jest": "^28.1.1",
    "jest-environment-jsdom": "^28.1.1",
    "prettier": "^2.6.2",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "rimraf": "^3.0.2",
    "snapshot-diff": "^0.8.1",
    "typescript": "^4.7.3"
  },
  "dependencies": {
    "ethers": "^5.6.8",
    "store2": "^2.13.2",
    "wagmi": "^0.5.5"
  },
  "lint-staged": {
    "*": "yarn lint"
  },
  "prettier": {
    "printWidth": 100,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  }
}
